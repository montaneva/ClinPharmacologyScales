    <surveyls_description><![CDATA[Naranjo CA, Busto U, Sellers EM, et al. A method for estimating the probability of adverse drug reactions. Clin Pharmacol Ther. 1981;30(2):239-45. <a class="external" href="http://www.ncbi.nlm.nih.gov/pubmed/7249508" target="pubmed">PMID: 7249508</a> <script type="text/javascript">

$(function(){

$( "<style type='text/css' media='screen and (min-width:481px)'>"

+ ".answers-wrapper li { display:inline;padding-right:20px !important; }"

+"</style>").appendTo( "head" );

});

</script>]]></surveyls_description>
    <surveyls_welcometext/>
    <surveyls_endtext><![CDATA[<script type="text/javascript">

var ACT = '{ASSESSMENT_CURRENT_TOTAL}';

if(ACT <= 1) { document.write('<style>.r0{ color:#fff;font-weight:bold;background:limegreen}</style>')};

if((ACT > 1)&&(ACT < 5)) { document.write('<style>.r1{ color:#fff;background:lightsteelblue}</style>')};

if((ACT >= 5)&&(ACT < 9)) { document.write('<style>.r2{ color:#fff;background:coral}</style>')};

if(ACT >= 9) { document.write('<style>.r3{ color:#fff;background:crimson}</style>')};

</script><center>
<table class="noprint" id="assess_tbl">
	<tbody>
		<tr class="head">
			<td>
				Score</td>
			<td>
				Adverse Drug Reaction</td>
		</tr>
		<tr class="r0">
			<td>
				<2</td>
			<td>
				Doubtful</td>
		</tr>
		<tr class="r1">
			<td>
				2 to 4</td>
			<td>
				Possible</td>
		</tr>
		<tr class="r2">
			<td>
				5 to 8</td>
			<td>
				Probable</td>
		</tr>
		<tr class="r3">
			<td>
				≥ 9</td>
			<td>
				Definite</td>
		</tr>
	</tbody>
</table></center>
]]></surveyls_endtext>
