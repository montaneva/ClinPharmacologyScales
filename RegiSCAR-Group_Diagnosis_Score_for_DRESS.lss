    <surveyls_welcometext/>
    <surveyls_endtext><![CDATA[<script type="text/javascript">



var ACT = '{ASSESSMENT_CURRENT_TOTAL}';



if(ACT < 2) { document.write('<style>.r0{ color:#fff;font-weight:bold;background:limegreen}</style>')};



if((ACT == 2)||(ACT == 3)) { document.write('<style>.r1{ color:#fff;background:lightsteelblue}</style>')};



if((ACT == 4)||(ACT == 5)) { document.write('<style>.r2{ color:#fff;background:coral}</style>')};



if(ACT >= 6) { document.write('<style>.r3{ color:#fff;background:crimson}</style>')};



</script><center>
<table class="noprint" id="assess_tbl">
	<tbody>
		<tr class="head">
			<td>
				Score</td>
			<td>
				DRESS</td>
		</tr>
		<tr class="r0">
			<td>
				<2</td>
			<td>
				None</td>
		</tr>
		<tr class="r1">
			<td>
				2-3</td>
			<td>
				Possible</td>
		</tr>
		<tr class="r2">
			<td>
				4-5</td>
			<td>
				Probable</td>
		</tr>
		<tr class="r3">
			<td>
				≥ 6</td>
			<td>
				Definite</td>
		</tr>
	</tbody>
</table></center>
]]></surveyls_endtext>
